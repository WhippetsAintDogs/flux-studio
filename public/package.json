{
    "main": "index.html?debug",
    "name": "Mozu Studio",
    "node-remote": ["http://*/*", "*://*/*"],
    "description": "Mozu Studio",
    "version": "{Version}",
    "inject_js_start": "nw-inject-start.js",
    "window": {
        "title": "Mozu Studio - {Version}",
        "icon": "icon.png",
        "frame": true,
        "width": 1280,
        "height": 640,
        "min_width": 1000,
        "min_height": 600
    },
    "nodejs": true,
    "chromium-args": "--mixed-context --enable-webgl --ignore-gpu-blacklist",
    "webkit": {
        "plugin": true
    },
    "manifestUrl": "http://software.flux3dp.com/check-update/?os=osx-x64&format=autoupdate"
}
