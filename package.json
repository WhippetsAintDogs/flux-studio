{
  "name": "flux-studio",
  "productName": "FLUX Studio",
  "version": "0.9.1",
  "author": {
    "name": "FLUX Inc.",
    "email": "dev@flux3dp.com"
  },
  "description": "FLUX Studio",
  "private": true,
  "main": "electron-main.js",
  "dependencies": {
    "jquery": "^1.11.0",
    "jsdom-global": "^1.6.2",
    "nan": "^2.4.0",
    "socket.io-client": "^1.4.6",
    "squirejs": "^0.2.1",
    "websocket": "~1.0.24"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "chai": "3.5.0",
    "electron": "~1.6.2",
    "electron-builder": "^19.7.1",
    "electron-publisher-s3": "^19.7.0",
    "electron-reload": "~1.1.0",
    "expect": "^1.14.0",
    "gulp": "^3.9.0",
    "gulp-babel": "~6.1.2",
    "gulp-clean-css": "~2.0.10",
    "gulp-exec": "~2.1.2",
    "gulp-mocha": "~2.2.0",
    "gulp-react": "~3.1.0",
    "gulp-sass": "~2.1.1",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-uglify": "~1.5.4",
    "gulp-webserver": "~0.9.1",
    "jsdom": "~7.2.2",
    "jsdom-global": "~1.6.1",
    "mocha": "^2.3.4",
    "mocha-jsdom": "~1.0.0",
    "pump": "~1.0.1",
    "q": "~1.4.1",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "requirejs": "~2.1.22",
    "squirejs": "~0.2.1",
    "style-loader": "^0.13.0",
    "webpack": "1.13.1"
  },
  "scripts": {
    "test": "mocha '_test/unit/slicer.js' --compilers js:babel-core/register",
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "com.flux3dp.flux-studio",
    "extraResources": [
      "backend"
    ],
    "files": [
      "!**/*.{jsx,scss}"
    ],
    "publish": [
      {
        "provider": "s3",
        "bucket": "${env.PUBLISH_BUCKET}",
        "path": "${os}${env.PUBLISH_PATH}"
      }
    ],
    "mac": {
      "category": "com.flux3dp.flux-studio",
      "target": "dmg",
      "icon": "public/logo.icns"
    },
    "linux": {
      "category": "Utility",
      "description": "",
      "target": "deb",
      "icon": "app-icons"
    },
    "win": {
      "icon": "app-icons/win-icon.ico",
      "certificateSubjectName": " "
    },
    "dmg": {
      "artifactName": "FLUX Studio ${version}.${ext}"
    },
    "nsis": {
      "include": "win-install.nsh",
      "artifactName": "FLUX Studio Installer ${version}.${ext}"
    }
  }
}
