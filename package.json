{
  "name": "mozu-studio",
  "productName": "Mozu Studio",
  "version": "1.0.0",
  "author": {
    "name": "FLUX Inc.",
    "email": "dev@flux3dp.com"
  },
  "description": "Mozu Studio",
  "private": true,
  "main": "electron-main.js",
  "dependencies": {
    "bufferpack": "^0.0.6",
    "font-manager": "^0.3.0",
    "fontkit": "^1.7.7",
    "jquery": "^1.11.0",
    "jsdom-global": "^1.6.2",
    "nan": "^2.10.0",
    "socket.io-client": "^1.4.6",
    "squirejs": "^0.2.1",
    "tar-fs": "^1.16.3",
    "text-to-svg": "jeffontheground/text-to-svg#e4fb3dc1ac9c8e5a2440373e7e018f07653ad574",
    "websocket": "~1.0.24"
  },
  "devDependencies": {
    "ajv": "^5.5.2",
    "babel-core": "^6.4.5",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "chai": "3.5.0",
    "electron": "^1.6.17",
    "electron-builder": "19.52.1",
    "electron-publisher-s3": "19.53.1",
    "electron-rebuild": "^1.8.1",
    "electron-reload": "~1.1.0",
    "eslint": "^4.9.0",
    "eslint-plugin-react": "^7.4.0",
    "expect": "^1.14.0",
    "gulp": "^3.9.0",
    "gulp-babel": "~6.1.2",
    "gulp-clean-css": "~2.0.10",
    "gulp-exec": "~2.1.2",
    "gulp-mocha": "~2.2.0",
    "gulp-react": "~3.1.0",
    "gulp-sass": "~2.1.1",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-uglify": "~1.5.4",
    "gulp-webserver": "~0.9.1",
    "jsdom": "~7.2.2",
    "mocha": "^2.3.4",
    "mocha-jsdom": "~1.0.0",
    "pump": "~1.0.1",
    "q": "~1.4.1",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "requirejs": "~2.1.22",
    "style-loader": "^0.13.0",
    "webpack": "1.13.1"
  },
  "scripts": {
    "test": "mocha '_test/unit/slicer.js' --compilers js:babel-core/register",
    "start": "electron .",
    "postinstall": "electron-rebuild",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "com.mozu.mozu-studio",
    "extraResources": [
      "backend"
    ],
    "files": [
      "!**/*.{jsx,scss}"
    ],
    "publish": [
      {
        "provider": "s3",
        "bucket": "${env.PUBLISH_BUCKET}",
        "path": "${os}${env.PUBLISH_PATH}"
      }
    ],
    "mac": {
      "category": "com.mozu.mozu-studio",
      "target": "dmg",
      "icon": "public/logo.icns"
    },
    "linux": {
      "category": "Graphics",
      "description": "Mozu Studio",
      "vendor": "FLUX Inc.",
      "target": "deb",
      "icon": "app-icons"
    },
    "win": {
      "icon": "app-icons/win-icon.ico",
      "certificateSubjectName": "FLUX TECHNOLOGY CORP.",
      "publisherName": "FLUX TECHNOLOGY CORP."
    },
    "dmg": {
      "artifactName": "Mozu Studio ${version}.${ext}"
    },
    "deb": {
      "depends": [
        "libfontconfig1-dev"
      ]
    },
    "nsis": {
      "include": "win-install.nsh",
      "artifactName": "Mozu Studio Installer ${version}.${ext}"
    }
  }
}
